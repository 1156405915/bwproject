                var num = $(this).offset().left-nav2L;
                var wid = $(this).width();
                $('.pub-line').css({'left':num,'width':wid});
            });
            $('.nav-deal').click(function(){
                $('html,body').animate({scrollTop:dealTop},800);
                var num = $(this).offset().left-nav2L;
                var wid = $(this).width();
                $('.pub-line').css({'left':num,'width':wid});
            });
            $('.nav-safe').click(function(){
