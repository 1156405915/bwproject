    <script>
        $(function() {
            //头部banner内部产品交互效果
            $('.pro_tab').mouseover(function() {
                    $(this).find('div').css({'opacity':'1','z-index':'-1'});
                })
            $('.pro_tab').mouseout(function() {
                    $(this).find('div').css({'opacity':'0.2','z-index':'0'})
                })
                //小额理财tab切换
            $('.small_tab li').mouseover(function() {
                var num = $(this).index();
                $('.small_tab li').removeClass('act');
                $(this).addClass('act');
                $('.pro').addClass('hidden');
                $('.pro').eq(num).removeClass('hidden');
            })
            //二级菜单锚点控制

            var navTop = $('.banner').offset().top;
            var smallTop = $('#small').offset().top;
            var monthTop = $('#month').offset().top;
            var dealTop = $('#deal').offset().top;
            var safeTop = $('#safe').offset().top;
            var nav2L = $('.pub-two-cont').offset().left;
            $('.nav-all').click(function(){
                $('html,body').animate({scrollTop:navTop},800);
                var num = $(this).offset().left-nav2L;
                var wid = $(this).width();
                $('.pub-line').css({'left':num,'width':wid});
            });
            $('.nav-small').click(function(){
                $('html,body').animate({scrollTop:smallTop},800);
                var num = $(this).offset().left-nav2L;
                var wid = $(this).width();
                $('.pub-line').css({'left':num,'width':wid});
            });
            $('.nav-month').click(function(){
